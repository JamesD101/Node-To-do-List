<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>ToDo List</title>
    <script type="text/javascript" src="/assets/jquery.min.js"> </script>
    <link href="/assets/styles.css" rel="stylesheet" type="text/css">
  </head>
  <body>
    <h1>My ToDo List</h1>
    <div id="todo-table">
      <form>
        <input id="form-input" type="text" name="item" placeholder="Add new item..." required />
        <button type="submit">Add Item</button>
      </form>
      <ul>
        <% for(var i = 0; i < todos.length; i++){ %>
            <li><%= todos[i].item %></li>
        <% } %>
      </ul>
    </div>
    <script type="text/javascript" src="my-todo-list.js">  </script>
  </body>
</html>
